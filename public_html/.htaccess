RewriteEngine On
RewriteRule ^s/([0-9]+)(/?)$ index.php?go=anasayfa&s=$1
RewriteRule ^blog/([0-9a-zA-Z-_]+).html(/?)$ index.php?go=blog&link=$1
RewriteRule ^([0-9a-zA-Z-_]+).html(/?)$ index.php?go=sayfa&link=$1
RewriteRule ^market/satinal/([0-9]+)(/?)$ index.php?go=market_satinal&uid=$1
RewriteRule ^market/([0-9a-zA-Z-_]+)(/?)$ index.php?go=market_sunucu&suid=$1
RewriteRule ^market/([0-9a-zA-Z-_]+)/([0-9a-zA-Z-_]+)(/?)$ index.php?go=market_kategori&suid=$1&kid=$2

RewriteRule ^destek/([0-9a-zA-Z-_]+)(/?)$ index.php?go=destek_$1
RewriteRule ^destek/s/([0-9]+)(/?)$ index.php?go=destek&s=$1
RewriteRule ^destek/duzenle/([0-9]+)(/?)$ index.php?go=destek_duzenle&tid=$1
RewriteRule ^destek/kapat/([0-9]+)(/?)$ index.php?go=destek_kapat&tid=$1

RewriteRule ^sifremi_unuttum/([0-9a-zA-Z]+)(/?)$ index.php?go=sifremi_unuttum&token=$1
RewriteRule ^kullan/([0-9a-zA-Z]+)(/?)$ index.php?go=kullan&uid=$1
RewriteRule ^([0-9a-zA-Z-_]+)/([0-9a-zA-Z-_]+)(/?)$ index.php?go=$1&link=$2

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([0-9a-zA-Z-_]+)(/?)$ index.php?go=$1

<IfDefine SSL>
  # RewriteCond %{SERVER_PORT} 80
  # RewriteRule ^.*$ https://%{SERVER_NAME}%{REQUEST_URI} [R=301,L]
</IfDefine>